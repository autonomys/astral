    "@heroicons/react": "^2.0.13",
    "@nivo/core": "^0.81.0",
    "@nivo/line": "^0.81.0",
    "@nivo/pie": "^0.81.0",
    "@polkadot/keyring": "^11.1.3",
    "@polkadot/react-identicon": "^2.11.2",
    "@polkadot/util": "^11.1.3",
    "@polkadot/util-crypto": "^11.1.3",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^16.11.68",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "bn.js": "^5.2.1",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.6",
    "eslint": "^8.0.1",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "lottie-react": "^2.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hot-toast": "^2.4.0",
    "react-json-view": "^1.21.3",
    "react-paginate": "^8.2.0",
    "react-router-dom": "6",
    "react-scripts": "5.0.1",
    "tailwind-datepicker-react": "^1.2.1",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:coverage": "CI=true yarn test -- --env=jsdom --coverage",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!@polkadot/keyring)/'",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.ts",
    "prestart": "yarn codegen",
    "predev": "yarn codegen",
    "prebuild": "yarn codegen",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint": "eslint . -c .eslintrc --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "4.0.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "autoprefixer": "^10.4.12",
    "babel-plugin-named-exports-order": "^0.0.2",
    "dotenv": "^16.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^4.0.0",
    "jest-canvas-mock": "^2.4.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.2.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "webpack": "^5.75.0"
  }
}
